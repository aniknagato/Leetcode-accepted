class Solution {
    public String reverseString(String s) {
        
        
        
        int len = s.length();
        
        if (len <2){
            return s;
        }
        StringBuilder res = new StringBuilder("");        
        for(int i=0;i<len;i++){
            
            String cur = Character.toString(s.charAt(i));
            
            res.insert(0,cur);
            
            // res = cur+res;
            
        
            
        }
        
        return res.toString();
        
    }
}
